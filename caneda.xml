<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
	<mime-type type="application/x-caneda-schematic">
		<sub-class-of type="text/plain"/>
		<comment>Caneda schematic file</comment>
		<icon>caneda</icon>
		<glob pattern="*.xsch"/>
	</mime-type>
	<mime-type type="application/x-caneda-symbol">
		<sub-class-of type="text/plain"/>
		<comment>Caneda symbol file</comment>
		<icon>caneda</icon>
		<glob pattern="*.xsym"/>
	</mime-type>
	<mime-type type="application/x-caneda-layout">
		<sub-class-of type="text/plain"/>
		<comment>Caneda layout file</comment>
		<icon>caneda</icon>
		<glob pattern="*.xlay"/>
	</mime-type>
	<mime-type type="application/x-circuit-simulation-raw">
		<sub-class-of type="text/plain"/>
		<comment>Circuit simulation raw file</comment>
		<icon>caneda</icon>
		<glob pattern="*.raw"/>
	</mime-type>
	<mime-type type="application/x-circuit-netlist">
		<sub-class-of type="text/plain"/>
		<comment>Circuit netlist file</comment>
		<icon>caneda</icon>
		<glob pattern="*.net"/>
	</mime-type>
</mime-info>
