<component name="Current Controlled Switch" version="0.1.0" label="S">
    <displaytext>
        <lang lang="C">Current Controlled Switch</lang>
        <lang lang="es">Llave Controlada por Corriente</lang>
    </displaytext>

    <description>
        <lang lang="C">Current controlled switch.</lang>
        <lang lang="es">Llave controlada por corriente.</lang>
    </description>

    <symbol>
        <painting name="line" line="0,0,10,0" pos="-20,0" transform="1,0,0,1,0,0"/>
        <painting name="line" line="0,0,15,-10" pos="-10,0" transform="1,0,0,1,0,0"/>
        <painting name="line" line="0,0,10,0" pos="10,0" transform="1,0,0,1,0,0"/>
    </symbol>

    <ports>
        <port name="+" pos="-20,0"/>
        <port name="-" pos="20,0"/>
    </ports>

    <properties>
        <property name="Vnam" default="name" unit="" visible="true">
            <description>
                <lang lang="C">Voltage source controlling current</lang>
            </description>
        </property>
        <property name="model" default="SW" unit="" visible="false">
            <description>
                <lang lang="C">Device model</lang>
                <lang lang="es">Modelo del dispositivo</lang>
            </description>
        </property>
        <property name="S0" default="OFF" unit="" visible="false">
            <description>
                <lang lang="C">Initial state when control voltage inside hysteresis range. Possible values: ON, OFF.</lang>
            </description>
        </property>
    </properties>

    <models>
        <model type="spice" syntax="W%label %port{+} %port{-} %property{Vnam} %property{model} %property{S0}
               %model{SW SW}"/>
    </models>
</component>
